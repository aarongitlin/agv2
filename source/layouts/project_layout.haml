!!! 5
- ie_html :lang => 'en', :class => 'no-js' do
  %head
    = partial "partials/document_head"

  %body{:class => current_page.data.body_class || 'page'}
    = partial 'partials/header'

    #main(role="main")
      .wrapper
        %h1= current_page.title
        = yield

    =# partial 'partials/footer'
    = partial 'partials/scripts'
